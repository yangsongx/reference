   "source": [
    "import datetime\n",
    "\n",
    "cur = datetime.datetime.now()\n",
    "yesterday = cur - datetime.timedelta(days = 1)\n",
    "tomorrow = cur + datetime.timedelta(days = 1)\n",
    "print(cur)\n",
    "print(yesterday)\n",
    "print(tomorrow)"
